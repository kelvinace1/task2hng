{
    "version": 2,
    "builds": [
      {
        "src": "hngorg/wsgi",
        "use": "@vercel/python",
        "config": {
          "maxLambdaSize": "15mb", 
          "runtime": "python3.9"
        }
      },
      {
        "src": "build.sh",
        "use": "@vercel/static-build",
        "config": {
          "distDir": "staticfiles_build"
        }
      }
    ],

    "rewrites": [
    { "source": "/(.*)", "destination": "/" }
  ]
    
    }
  